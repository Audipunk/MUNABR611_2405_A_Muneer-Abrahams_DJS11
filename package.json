      "dev:js": "yarn dev:js:cjs & yarn dev:js:esm",
      "dev:js:cjs": "tsc --project tsconfig-cjs.json --watch",
      "dev:js:esm": "tsc --project tsconfig-esm.json --watch",
      "dev:css": "postcss src/styles/index.css -o styles.css --watch",
